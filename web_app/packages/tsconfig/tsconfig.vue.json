{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"compilerOptions": {
		"composite": true,
		"lib": ["esnext", "dom"],
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"skipLibCheck": true,
		"jsxImportSource": "vue",
		"allowJs": false,
	}
}
