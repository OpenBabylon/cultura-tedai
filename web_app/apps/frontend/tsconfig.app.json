{
	"extends": "@cultura-ai/tsconfig/tsconfig.vue.json",
	"include": ["env.d.ts", "components.d.ts", "typed-router.d.ts", "src/**/*", "src/**/*.vue"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"composite": true,
		"skipLibCheck": true,
		"moduleResolution": "Bundler",
		"tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
